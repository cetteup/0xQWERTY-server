{
  "name": "qwerty-server",
  "version": "1.0.1",
  "description": "Server for Twitch viewer keyboard",
  "main": "dist/server.js",
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build-ts": "tsc -b",
    "postinstall": "npm run build-ts",
    "start": "npm run serve",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "test": "npx eslint \"src/**\" --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cetteup/qwerty-server.git"
  },
  "author": "cetteup (me@cetteup.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cetteup/qwerty-server/issues"
  },
  "homepage": "https://github.com/cetteup/qwerty-server#readme",
  "dependencies": {
    "@hapi/boom": "^9.1.1",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "nanoid": "^3.2.0",
    "socket.io": "^3.1.0",
    "socket.io-client": "^3.1.1",
    "tmi.js": "^1.7.1",
    "tslog": "^3.3.1"
  },
  "devDependencies": {
    "@tsconfig/recommended": "1.0.1",
    "@types/express": "4.17.14",
    "@types/hapi": "18.0.5",
    "@types/socket.io": "2.1.13",
    "@types/socket.io-client": "1.4.35",
    "@types/tmi.js": "1.7.1",
    "@types/ws": "7.4.0",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "eslint": "7.18.0",
    "nodemon": "2.0.20",
    "typescript": "4.1.3"
  }
